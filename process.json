{
  "apps": [
    {
      "name": "vuejs2-demo",
      "script": "server.js",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "instances": "0",
      "watch": "false",
      "ignoreWatch": [
        "[\\/\\\\]\\./",
        "node_modules"
      ],
      "merge_logs": true,
      "exec_mode": "cluster_mode",
      "node_args": "--harmony --use-strict",
      "env_dev": {
        "NODE_ENV": "dev"
      },
      "env_prod": {
        "NODE_ENV": "prod"
      }
    }
  ]
}